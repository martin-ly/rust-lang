{
  "title": "Async API Gateway - Overview",
  "panels": [
    { "type": "timeseries", "title": "QPS", "targets": [{ "expr": "rate(gateway_request_total[1m])" }] },
    { "type": "timeseries", "title": "Latency P95", "targets": [{ "expr": "histogram_quantile(0.95, rate(gateway_request_seconds_bucket[5m]))" }] },
    { "type": "barchart", "title": "Rate Limited by Reason", "targets": [{ "expr": "increase(gateway_rate_limited_total[5m]) by (reason)" }] },
    { "type": "barchart", "title": "Backend Status", "targets": [{ "expr": "increase(gateway_backend_status_total[5m]) by (status)" }] },
    { "type": "barchart", "title": "Instance Picks", "targets": [{ "expr": "increase(gateway_instance_pick_total[5m]) by (instance)" }] }
  ],
  "time": { "from": "now-1h", "to": "now" },
  "templating": { "list": [] }
}

